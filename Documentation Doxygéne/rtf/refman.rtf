125 \par
126     m_pPollTimeout.stop();\par
127     m_nLinkTries = 0;\par
128     emit XBeeStatus(true);\par
129 \par
130     *(log->getSystemLoggerStream()) << endl << "Compute polled data" << endl;\par
131 \par
132     while (index < length)\par
133     \{\par
134         *(log->getSystemLoggerStream()) << "    End of buffer not reached : number of resting char = " << length - index << endl;\par
135 \par
136         while (!(data[index] == '>'))\par
137         \{\par
138             index++;\par
139         \}\par
140 \par
141         if ((data[index + 1] == 'a' && data[index + 3] == 'a' && data[index + 4] == '<') && index < length)\par
142         \{\par
143             if (index <= 395)\par
144             \{\par
145                 *(log->getSystemLoggerStream()) << "Command acknowledge received" << endl;\par
146                 memcpy((void*)ack, (void*)&data[index], 5 * sizeof(char));\par
147                 ack[5] = '\\0';\par
148             \}\par
149 \par
150             emit displayAck(ack);\par
151         \}\par
152         else if ((data[index + 1] == '*' && data[index + 2] == '>') && index < length - sizeof(">*>") - 1 - sizeof(short))\par
153         \{\par
154             *(log->getSystemLoggerStream()) << "    New polled data structure found at index " << index << endl;\par
155 \par
156             descriptor = data[index + sizeof(">*>") - 1 + sizeof(short)];\par
157             index += sizeof(">*>") - 1 + sizeof(short) + sizeof(char);\par
158 \par
159             if (descriptor == STATUS_DESCRIPTOR)\par
160             \{\par
161                 *(log->getSystemLoggerStream()) << "        Status polled data found in buffer" << endl;\par
162 \par
163                 if (index < (length - sizeof(LL_STATUS) - sizeof(short) + sizeof("<#<") - 1))\par
164                 \{\par
165                     memcpy((void*)&st, &data[index], sizeof(LL_STATUS));\par
166 \par
167                     index += sizeof(LL_STATUS);\par
168                     memcpy((void*)&sendCrc, &data[index], sizeof(short));\par
169 \par
170                     checksum = crc16(&st, sizeof(LL_STATUS));\par
171 \par
172                     emit batteryLevel(st.battery_voltage_1 / 1000.);\par
173                     emit updateStatus(st.flightMode);\par
174                     \par
175                     emit inFlight(st.flying);\par
176                 \}\par
177                 else\par
178                 \{\par
179                     *(log->getSystemLoggerStream()) << "        ERROR : Incomplete structure" << endl;\par
180                 \}\par
181             \}\par
182             else if (descriptor == IMU_CALCDATA_DESCRIPTOR)\par
183             \{\par
184                 *(log->getSystemLoggerStream()) << "        IMU polled data found in buffer" << endl;\par
185 \par
186                 if (index < (length - sizeof(IMU_CALCDATA) - sizeof(short) + sizeof("<#<") - 1))\par
187                 \{\par
188                     memcpy((void*)&imu, &data[index], sizeof(IMU_CALCDATA));\par
189 \par
190                     index += sizeof(IMU_CALCDATA);\par
191                     memcpy((void*)&sendCrc, &data[index], sizeof(short));\par
192 \par
193                     checksum = crc16(&imu, sizeof(IMU_CALCDATA));\par
194                     heading = imu.mag_heading / 1000.;\par
195                     height = imu.height / 1000.;\par
196 \par
197                     emit updateHeading(heading);\par
198                     emit updateHeight(height);\par
199                 \}\par
200                 else\par
201                 \{\par
202                     *(log->getSystemLoggerStream()) << "        ERROR : Incomplete structure" << endl;\par
203                 \}\par
204             \}\par
205             else if (descriptor == CURRENT_WAY_DESCRIPTOR)\par
206             \{\par
207                 *(log->getSystemLoggerStream()) << "        Current way polled data found in buffer" << endl;\par
208 \par
209                 if (index < (length - sizeof(CURRENT_WAY) - sizeof(short) + sizeof("<#<") - 1))\par
210                 \{\par
211                     memcpy((void*)&cw, &data[index], sizeof(CURRENT_WAY));\par
212 \par
213                     index += sizeof(CURRENT_WAY);\par
214                     memcpy((void*)&sendCrc, &data[index], sizeof(short));\par
215 \par
216                     checksum = crc16(&cw, sizeof(CURRENT_WAY));\par
217 \par
218                     emit waypointNAVInfo(cw.current_wp, cw.distance_to_wp / 10., cw.navigation_status);\par
219                 \}\par
220                 else\par
221                 \{\par
222                     *(log->getSystemLoggerStream()) << "        ERROR : Incomplete structure" << endl;\par
223                 \}\par
224             \}\par
225             else if (descriptor == GPS_DESCRIPTOR && index < (length - sizeof(GPS_DATA_ADVANCED) - sizeof(short) + sizeof("<#<") - 1))\par
226             \{\par
227                 *(log->getSystemLoggerStream()) << "        Geolocation polled data found in buffer" << endl;\par
228 \par
229                 if (index < (length - sizeof(GPS_DATA_ADVANCED) - sizeof(short) + sizeof("<#<") - 1))\par
230                 \{\par
231                     memcpy((void*)&gps, &data[index], sizeof(GPS_DATA_ADVANCED));\par
232 \par
233                     index += sizeof(GPS_DATA_ADVANCED);\par
234                     memcpy((void*)&sendCrc, &data[index], sizeof(short));\par
235 \par
236                     checksum = crc16(&gps, sizeof(GPS_DATA_ADVANCED));\par
237 \par
238                     longitude = gps.longitude / 10000000.;\par
239                     latitude = gps.latitude / 10000000.;\par
240                     altitude = gps.height / 1000.;\par
241                     double speedX = qAbs(gps.speed_x * 0.0036);\par
242                     double speedY = qAbs(gps.speed_y * 0.0036);\par
243                     m_dSpeed = qSqrt(speedX * speedX + speedY * speedY);\par
244 \par
245                     if (checksum == sendCrc)\par
246                     \{\par
247                         emit geolocation(latitude, longitude, altitude, heading);\par
248                         emit updateSpeed(m_dSpeed);\par
249                         emit updateGPSQuality(gps.numSV);\par
250                     \}\par
251                 \}\par
252                 else\par
253                 \{\par
254                     *(log->getSystemLoggerStream()) << "        ERROR : Incomplete structure" << endl;\par
255                 \}\par
256 \par
257                 writeLog(checksum == sendCrc);\par
258             \}\par
259             else if (descriptor == RC_DATA_DESCRIPTOR && index < (length - sizeof(RC_DATA) - sizeof(short) + sizeof("<#<") - 1))\par
260             \{\par
261                 *(log->getSystemLoggerStream()) << "        RC polled data found in buffer" << endl;\par
262 \par
263                 if (index < (length - sizeof(RC_DATA) - sizeof(short) + sizeof("<#<") - 1))\par
264                 \{\par
265                     memcpy((void*)&rc, &data[index], sizeof(RC_DATA));\par
266 \par
267                     index += sizeof(RC_DATA);\par
268                     memcpy((void*)&sendCrc, &data[index], sizeof(short));\par
269 \par
270                     checksum = crc16(&rc, sizeof(RC_DATA));\par
271 \par
272                     emit rcData(rc.channels_out[3], rc.channels_out[0], rc.channels_out[1], \par
273                             rc.channels_out[2], rc.channels_out[4], rc.channels_out[5], rc.lock);\par
274                 \}\par
275                 else\par
276                 \{\par
277                     *(log->getSystemLoggerStream()) << "        ERROR : Incomplete structure" << endl;\par
278                 \}\par
279             \}\par
280             else\par
281             \{\par
282                 *(log->getSystemLoggerStream()) << "        Unknown polled data type found in buffer" << endl;\par
283             \}\par
284         \}\par
285         index += sizeof(short) + sizeof("<#<") - 1;\par
286     \}\par
287 \par
288     *(log->getSystemLoggerStream()) << "Polled data sent" << endl;\par
289 \}\par
}
